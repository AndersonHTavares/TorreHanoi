<!DOCTYPE html>
<html ng-app="app">
<head>
    <title>@ViewBag.Title</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/album.css" rel="stylesheet" />
    <link href="@Url.Content("~/Content/torreHanoi.css")" rel="StyleSheets" type="text/css" />
    <script src="@Url.Content("~/Scripts/torreHanoi.js")" type="text/javascript"></script>
    <script src="~/Scripts/jquery-3.1.1.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/SimuladorHanoi.js"></script>

    <script src="~/Scripts/angular.js"></script>

    <script src="http://ui-grid.info/docs/grunt-scripts/vfs_fonts.js"></script>

    <script src="~/Scripts/ui-grid.js"></script>

    @*<link rel="stylesheet" href="http://ui-grid.info/release/ui-grid.css" type="text/css">*@


    <link href="~/Content/ui-grid.css" rel="stylesheet" />

    <script src="~/Scripts/app.js"></script>

    <style>
        

        div.containerstacks {
            margin-right: 140px;
            margin-top: 35px;
            max-width: 35em;
            margin-left: auto;
            /*margin-right: initial;*/
        }

        #stacks {
            width: 98%;
            margin-left: auto;
            margin-right: auto;
            margin-bottom: 1em;
            text-align: center;
        }

        .stack {
            width: 30%;
            margin-right: 1%;
            display: inline-block;
        }

        .disc {
            display: block;
            height: 2em;
            line-height: 3em;
            text-align: center;
            width: 100%;
            background-color: #EEEEEE;
            margin-left: auto;
            margin-right: auto;
            margin-top: 0.3em;
            overflow: hidden;
            transition: background-color 0.18s ease-in;
        }

        .selected {
            background-color: #AAAAFF;
        }


        #divhistorico {
            height: 150px;
            overflow-y: auto;
            list-style-type: none;
            margin-top: 29px;
        }
    </style>

</head>
<body>
    @RenderBody()
</body>
</html>
